{
  "name": "youtrack",
  "version": "0.0.1",
  "description": "Personal drag'n'drop task tracker",
  "author": "George Shinkarev <gkshi@ya.ru>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build --modern",
    "now-build": "npm run build",
    "start": "cross-env NODE_ENV=production node server/index.js --modern",
    "generate": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.7",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "mongodb": "^3.5.6",
    "nuxt": "^2.12.2"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-nuxt": ">=0.5.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "nuxt-models": "^0.0.9",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.2",
    "sortablejs": "^1.10.2"
  }
}
