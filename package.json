{
  "name": "you-track",
  "version": "1.3.0",
  "description": "Personal drag'n'drop task tracker",
  "author": "George Shinkarev",
  "private": true,
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build --modern",
    "build:dev": "NODE_ENV=develop nuxt build --modern",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:local": "cross-env NODE_ENV=develop node server/index.js",
    "generate": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "body-parser": "^1.19.0",
    "cron": "^1.8.2",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "firebase": "^8.0.0",
    "firebase-admin": "^9.3.0",
    "mongodb": "^3.6.2",
    "multer": "^1.4.2",
    "nuxt": "^2.14.7",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^4.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-plugin-nuxt": ">=1.0.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "nuxt-models": "^0.0.11",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.0.5",
    "sortablejs": "1.10.2",
    "vue-common-components": "^0.0.16"
  }
}
