{
  "name": "you-track",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(js|css|img)/.*",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/" }
  ],
  "env": {
    "db_username": "@db_username",
    "db_password": "@db_password",
    "secret1": "value1"
  }
}
